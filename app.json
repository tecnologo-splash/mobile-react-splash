{
  "expo": {
    "name": "Splash Mobile",
    "slug": "mobile-react-splash",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon_dark.png",
    "splash": {
      "image": "./assets/icon_dark.png",
      "resizeMode": "contain",
      "backgroundColor": "#6f32c1"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon_dark.png",
        "backgroundColor": "#6f32c1"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
